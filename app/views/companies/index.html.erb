<h1>All companies</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
    </tr>
  </thead>
  
  <tbody>
    <% for company in @companies %>
    <tr>
      <td><%= company.name %></td>
      <td><%= link_to "Show", company_path(company) %></td>
    </tr>
    <% end %>
  </tbody>
</table>
